@* @model dynamic *@
@model System.Collections.Generic.List<ExcelOperationsMvc.Models.User>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Users</h2>
<table>
        <thead>
        <tr>
            <th>FirstName</th>
            <th>LastName</th>
            <th>Age</th>
            <th>Address</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var user in Model!)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Age</td>
                <td>@user.Address</td>
            </tr>
        }
        </tbody>
    </table>

